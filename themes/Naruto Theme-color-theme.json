{
	"name": "Naruto Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#2b2a26",
		"editor.selectionBackground": "#2c2d39e7",
		"editor.foreground": "#fff",
		"panel.background": "#0b1679",
		"editor.hoverHighlightBackground": "#d12121",
		"editorBracketMatch.border": "#d12121",
		"panel.border": "#d12121",
		"panelTitle.activeBorder":"#fea22d",
		"panelTitle.activeForeground": "#fff",
		"panelTitle.inactiveForeground": "#bbbdcf",
		"terminal.background": "#2d2c2c",
		"statusBar.background": "#d12121",
		"breadcrumb.foreground":  "#fff",
		"breadcrumb.background": "#0b1679",
		"titleBar.activeBackground": "#d12121",
		"breadcrumb.focusForeground": "#bbbdcf",
		"breadcrumbPicker.background": "#d12121",
		"contrastBorder": "#d12121",
		"focusBorder": "#d12121",
		"editorLineNumber.foreground": "#bbbdcf",
		"editorLineNumber.activeForeground": "#9a9891",
		"editorCursor.background": "#d12121",
		"editorCursor.foreground": "#d12121",
		"scrollbarSlider.activeBackground": "#d12121",
		"scrollbarSlider.hoverBackground": "#d12121",
		"scrollbarSlider.background": "#fea22d",
		"progressBar.background": "#d12121",
		"scrollbar.shadow": "#fea22d",
		"button.background": "#d12121",
		"editorRuler.foreground": "#d12121",
		"activityBar.border": "#fe1212",
		"activityBar.foreground": "#fff",
		"activityBar.background":  "#0b1679",
		"activityBar.inactiveForeground": "#bbbdcf",
		"activityBarBadge.background": "#fe1212",
		"tab.activeBackground": "#fea22d",
		"tab.unfocusedActiveBackground": "#bbbdcf",
		"tab.border": "#2c2d39",
		"tab.hoverBackground": "#bbbdcf",
		"tab.hoverBorder": "#fe1212",
		"tab.activeBorder": "#fe1212",
		"tab.activeBorderTop": "#fe1212",
		"tab.unfocusedActiveBorder": "#fff",
		"editorPane.background": "#fe1212",
		"list.activeSelectionBackground": "#d12121",
		"list.hoverBackground": "#fe1212",
		"list.inactiveSelectionBackground": "#fe1212",
		"list.inactiveSelectionForeground": "#fff",
		"list.highlightForeground": "#fff",
		"list.focusForeground": "#fff",
		"list.hoverForeground": "#fff",
		"list.inactiveFocusBackground": "#b8b7b4",
		"sideBar.background": "#fea22d",
		"sideBar.foreground": "#240303",
		"sideBar.border": "#425eed",
		"sideBarTitle.foreground":"#270707",
		"sideBar.dropBackground": "#fe1212",
		"sideBarSectionHeader.background": "#0b1679",
		"sideBarSectionHeader.foreground": "#f5f5f5",
		"sideBarSectionHeader.border": "#425eed",
		"editorIndentGuide.background": "#240303",
		// "editor.fontFamily": "'Ninja Naruto', Arial"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#e1e1e1"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#e1e1e1"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#a3ff7d",
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#ff5050"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#7dc6ff"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#a3b7f2"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#fe31f2"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#ffaa50"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#f2d6b3"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff5050"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#ff0"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#ffaa50"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#a3ff7d"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#fe895e"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#7dc6ff"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#f6a94c"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#d2cad2"
			}
		},
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#e1e1e1"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#91B3E0"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#7dc6ff"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#91B3E0"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#f6a94c"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#fe31f2"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#f6a94c"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#a3ff7d"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"foreground": "#ff5050"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#7dc6ff"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#d2cad2"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#d2cad2"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#ff5050"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "#ff5050"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#fe31f2"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#7dc6ff"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#a3ff7d"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#f6a94c"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#ffdab9"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#ffdab9"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#fff"
			}
		}
	]
}